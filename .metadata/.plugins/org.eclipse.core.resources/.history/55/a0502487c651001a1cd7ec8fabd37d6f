import java.util.Scanner;
import java.security.SecureRandom;


public class CAI1 {

	
	static int random_int1;
	static int random_int2;
	static Scanner scan = new Scanner(System.in);
	static int correctAns = askQuestion();
	static int userAns;
	Boolean isCorrect = true;
	
	public static void quiz()
	{
		
		isAnswerCorrect(); 
	}	
	
	public static int askQuestion()
	{
		SecureRandom rand= new SecureRandom();
		random_int1 = rand.nextInt(10);
		random_int2 = rand.nextInt(10);
		
		System.out.println("How much is " + random_int1+ " times " + random_int2 + "?");
		
		return random_int1*random_int2;
	}
	
	public static int readResponse(int ans)
	{
		ans = userAns;
		System.out.print("\n Your answer:");
		userAns = scan.nextInt();
	
		return ans; 
		
	}
	
	public static void isAnswerCorrect()
	{
		if(readResponse(correctAns) == correctAns)
		{
			System.out.print("Good");
		}
		else
		{
			System.out.print("wring");
		}
		
		
	}
	
	public static void displayCorrectResponse()
	{
		
	}
	
	public static void displayIncorrectResponse()
	{
		
	}
	
	
	
	
	
	public static void main(String[] args) 
	{
		quiz();

	}

}
